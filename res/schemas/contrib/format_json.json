{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "ensure_ascii": {
      "type": "boolean"
    },
    "allow_nan": {
      "type": "boolean"
    },
    "indent": {
      "type": "integer"
    },
    "separators": {
      "type": "array",
      "items": {
        "type": "string",
        "minLength": 1
      },
      "minItems": 2,
      "maxItems": 2,
      "uniqueItems": true
    },
    "sort_keys": {
      "type": "boolean"
    },
    "final_newline": {
      "type": "boolean"
    }
  }
}
